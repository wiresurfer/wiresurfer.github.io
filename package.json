{
  "name": "wiresurfer-blog",
  "version": "1.0.0",
  "description": "Shaishav Kr's blog built with MkDocs Material",
  "scripts": {
    "build:css": "tailwindcss -i ./docs/assets/css/tailwind-input.css -o ./docs/assets/css/tailwind.css --minify",
    "watch:css": "tailwindcss -i ./docs/assets/css/tailwind-input.css -o ./docs/assets/css/tailwind.css --watch",
    "dev": "npm run watch:css & mkdocs serve",
    "build": "npm run build:css && mkdocs build"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.0"
  },
  "keywords": [
    "blog",
    "mkdocs",
    "tailwindcss"
  ],
  "author": "Shaishav Kr",
  "license": "MIT"
}
